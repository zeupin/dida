{
  "name": "dida/framework",
  "description": "Dida Framework",
  "type": "library",
  "homepage": "https://github.com/zeupin/dida",
  "keywords": [
    "Dida",
    "Framework"
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Dida\\": "src/Dida"
    }
  },
  "autoload-dev": {},
  "scripts": {
    "cs": "php-cs-fixer fix .",
    "dida:merge": "php bin/merge_dida_parts.php"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "authors": [
    {
      "name": "Macc Liu",
      "email": "mail@maccliu.com"
    }
  ],
  "license": "MIT"
}
